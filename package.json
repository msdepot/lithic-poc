{
  "name": "lithic-poc",
  "version": "1.0.0",
  "description": "Lithic Card Issuing POC with Admin CRM and User Management",
  "scripts": {
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "supabase:start": "cd backend && supabase start",
    "supabase:stop": "cd backend && supabase stop",
    "supabase:status": "cd backend && supabase status"
  },
  "keywords": ["lithic", "card-issuing", "fintech", "poc"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
